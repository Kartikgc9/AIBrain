{
  "name": "aibrain-server",
  "version": "1.0.0",
  "description": "AIBrain Remote Backend Server with PostgreSQL + pgvector",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "drizzle-kit push:pg",
    "migrate:generate": "drizzle-kit generate:pg",
    "test": "vitest"
  },
  "keywords": [
    "aibrain",
    "memory",
    "fastify",
    "postgresql",
    "pgvector"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^10.0.0",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.25.1",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2",
    "openai": "^4.20.1",
    "pino-pretty": "^10.3.1",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "drizzle-kit": "^0.20.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
